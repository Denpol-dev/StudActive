                </Button>
                <Button x:Name="CloseWin" Width="25" Height="25" HorizontalAlignment="Right" Margin="5" Background="Red" BorderBrush="Red" Style="{StaticResource MaterialDesignFloatingActionButton}" Click="CloseWin_Click">
